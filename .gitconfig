[user]
    name = Bill Jameson
    email = williamjameson@alum.rpi.edu
[core]
    excludesfile = /home/eltoraz/.gitignore_global
    editor = vim

[color]
    diff = auto
    status = auto
    branch = auto
    interactive = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan

[merge]
    conflictstyle = diff3

[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log --oneline --graph --decorate

[push]
	defaut = simple
